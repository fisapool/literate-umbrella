{
  "title": "NSR Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "URLs to start scraping from. Leave empty to scrape all states.",
      "editor": "requestListSources",
      "default": [],
      "prefill": []
    },
    "states": {
      "title": "States to Scrape",
      "type": "array",
      "description": "List of Malaysian states to scrape. Leave empty to scrape all states.",
      "editor": "select",
      "items": {
        "type": "string",
        "enum": [
          "johor",
          "kedah",
          "kelantan",
          "melaka",
          "negeri sembilan",
          "pahang",
          "perak",
          "perlis",
          "pulau pinang",
          "sabah",
          "sarawak",
          "selangor",
          "terengganu",
          "kuala lumpur",
          "labuan",
          "putrajaya"
        ]
      },
      "default": []
    },
    "maxConcurrency": {
      "title": "Max Concurrency",
      "type": "integer",
      "description": "Maximum number of pages to scrape concurrently",
      "default": 5,
      "minimum": 1,
      "maximum": 50
    },
    "maxRequestsPerCrawl": {
      "title": "Max Requests",
      "type": "integer",
      "description": "Maximum number of pages to scrape (0 = unlimited)",
      "default": 0,
      "minimum": 0
    },
    "proxyConfiguration": {
      "title": "Proxy Configuration",
      "type": "object",
      "description": "Proxy settings for the scraper",
      "editor": "proxy",
      "default": {
        "useApifyProxy": true
      }
    }
  }
}
